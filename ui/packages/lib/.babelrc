{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": [
          ">0.2%",
          "not dead",
          "not op_mini all"
        ]
      },
      "modules": false
    }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-export-default-from"
  ]
}
